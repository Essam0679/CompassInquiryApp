{
  "expo": {
    "name": "Compass Ocean Logistics",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "compassoceanlogistics",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.compassoceanlogistics.app",
      "buildNumber": "3",
      "displayName": "CompassLog",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to scan documents and take photos for shipping documentation.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photos for uploading shipping documentation.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to photos to save shipping documentation."
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:compassoceanlogistics.com"
      ]
    },
    "android": {
      "package": "com.compassoceanlogistics.app",
      "buildType": "app-bundle",
      "versionCode": 2,
      "displayName": "CompassLog",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "./withRemoveCameraPermission.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "456c7d0c-d950-4071-90ed-1b71aaf6a645"
      }
    }
  }
}
