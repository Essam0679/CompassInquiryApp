{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
    // No other compilerOptions needed here that might conflict with "extends"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "types/**/*.d.ts" // <--- THIS IS THE ONLY LINE YOU NEED TO ADD/MODIFY
  ]
}
